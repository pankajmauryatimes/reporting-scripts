export to /home/datareq/generic_etl/logs/experience_breakup.txt of del modified by coldel, datesiso  select trim(char(t.experience_rang))||left( ' ', 20 - length(trim(char(t.experience_rang))) )||':'||char(t.count_resume) from(select CASE WHEN rra.WORK_EXP between 0 and 2 then 'Work_exp_0_2' WHEN rra.WORK_EXP between 3 and 5 then 'Work_exp_3_5' WHEN rra.WORK_EXP between 6 and 10 then 'Work_exp_6_10' WHEN rra.WORK_EXP > 10 then 'Work_exp_10_plus' ELSE 'NOT DEFINED' END as experience_rang, count(1) as count_resume from SITE_INBOX_RESPONSE sir, RCRT_RESUME_ADS rra where sir.date_responded = current date - 1 day and rra.ad_id= sir.resume_id group by CASE WHEN rra.WORK_EXP between 0 and 2 then 'Work_exp_0_2' WHEN rra.WORK_EXP between 3 and 5 then 'Work_exp_3_5' WHEN rra.WORK_EXP between 6 and 10 then 'Work_exp_6_10' WHEN rra.WORK_EXP > 10 then 'Work_exp_10_plus' ELSE 'NOT DEFINED' END)t with ur

export to /home/datareq/generic_etl/logs/fa_breakup.txt of del modified by coldel, datesiso select trim(char(t.FUNCTION))||left( ' ', 20 - length(trim(char(t.FUNCTION))) )||':'||char(t.job_count) from( select (CASE WHEN (rja.job_function like '%35%' OR rja.job_function like '%36%') THEN 'IT' else 'NOT_IT' END) as FUNCTION , count(1) as job_count from SITE_INBOX_RESPONSE sir, rcrt_job_ads rja where sir.date_responded = current date - 1 day and rja.ad_id= sir.ad_id Group by (CASE WHEN (rja.job_function like '%35%' OR rja.job_function like '%36%') THEN 'IT' else 'NOT_IT' END))t with ur 

